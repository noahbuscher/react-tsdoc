{
  "name": "react-tsdoc",
  "version": "1.2.0",
  "author": "Noah Buscher",
  "description": "CLI to extract information from React Typescript component files with TSDoc for documentation generation purposes",
  "repository": {
    "type": "git",
    "url": "https://github.com/noahbuscher/react-tsdoc"
  },
  "license": "MIT",
  "keywords": [
    "documentation-generation",
    "react",
    "tsdoc",
    "typescript"
  ],
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "lib/main.js",
  "bin": {
    "react-tsdoc": "./bin/react-tsdoc.js"
  },
  "scripts": {
    "build": "rimraf lib/ && npm run build:ts",
    "build:ts": "tsc --project tsconfig.build.json",
    "test": "jest"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "ts-morph": "^13.0.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
