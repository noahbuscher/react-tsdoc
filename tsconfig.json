{
	"compilerOptions": {
		"esModuleInterop": true,
		"moduleResolution": "node",
		"target": "es5",
		"module": "commonjs",
		"outDir": "./lib",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"suppressImplicitAnyIndexErrors": true,
	},
	"exclude": [
		"node_modules"
	],
	"files": [
		"src/parser.ts"
	]
}
